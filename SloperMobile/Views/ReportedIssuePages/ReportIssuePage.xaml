<?xml version="1.0" encoding="utf-8" ?>
<CarouselPage
	x:Class="SloperMobile.Views.ReportedIssuePages.ReportIssuePage"
	xmlns="http://xamarin.com/schemas/2014/forms"
	xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
	xmlns:UC="clr-namespace:SloperMobile.UserControls.ReportIssue;assembly=SloperMobile"
	xmlns:ios="clr-namespace:Xamarin.Forms.PlatformConfiguration.iOSSpecific;assembly=Xamarin.Forms.Core"
	xmlns:prism="clr-namespace:Prism.Mvvm;assembly=Prism.Forms"
	prism:ViewModelLocator.AutowireViewModel="True"
	NavigationPage.HasNavigationBar="True"
	Style="{StaticResource AppBackgroundColor}">
	<ContentPage Style="{DynamicResource AppBackgroundColor}"
                 ControlTemplate="{StaticResource HeaderFooterPageTemplate}">
		<UC:IssueToReport
            x:Name="isuCatType"/>
	</ContentPage>
</CarouselPage>